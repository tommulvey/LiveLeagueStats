#!/Users/alex/Desktop/Test/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'streamlink==1.3.1','console_scripts','streamlink'
__requires__ = 'streamlink==1.3.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('streamlink==1.3.1', 'console_scripts', 'streamlink')()
    )
